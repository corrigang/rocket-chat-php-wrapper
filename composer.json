{
	"name": "viper-vlad/rocket-chat-php-wrapper",
	"description": "Rocket Chat REST API PHP Wrapper Library",
	"keywords": ["rocketchat", "rest", "api"],
	"homepage": "https://github.com/alekseykuleshov/rocket-chat",
	"license": "MIT",
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.4",
		"guzzlehttp/guzzle": "~7.0",
		"egulias/email-validator": "~2.0",
		"doctrine/collections": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.0",
		"codeception/aspect-mock": "~3.0",
		"friendsofphp/php-cs-fixer": "^2.16"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
